<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- Define custom configuration section handlers here if needed -->
  </configSections>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  
  <appSettings>
    <add key="DefaultTheme" value="Light" />
    <add key="AutoSaveIntervalMinutes" value="5" />
  </appSettings>
  
  <connectionStrings>
    <add name="ExcelDataConnection" 
         connectionString="Data Source=.;Initial Catalog=ExcelData;Integrated Security=True" 
         providerName="System.Data.SqlClient" />
  </connectionStrings>
  
  <system.diagnostics>
    <!-- Configure logging and debugging settings here -->
  </system.diagnostics>
  
  <runtime>
    <!-- Add assembly binding redirects here if needed -->
  </runtime>
</configuration>